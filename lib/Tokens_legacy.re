[@deriving show]
type token =
  | KEYWORD(string)
  | FUNCTION(string)
  | INT(int)
  | LOWER_THAN
  | GREATER_THAN
  | QUOTE
  | DOUBLE_AMPERSAND
  | DOUBLE_BAR
  | BAR
  | LEFT_BRACKET
  | RIGHT_BRACKET
  | ASTERISK
  | PLUS
  | QUESTION_MARK
  | LEFT_BRACE
  | RIGHT_BRACE
  | COMMA
  | HASH
  | EXCLAMATION_POINT
  | LEFT_PARENS
  | RIGHT_PARENS
  | SLASH
  | EOF;
